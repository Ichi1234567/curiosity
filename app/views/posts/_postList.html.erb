  <% list.each do |post| %>
    <tr id = post_<%= post.id %>>
      <td><%= post.title %></td>
      <td><%= post.content %></td>
      <td><%= link_to 'Show', post %></td>
      <td><%= link_to 'Edit', edit_post_path(post) %></td>
      <td><%= link_to 'Destroy', post, method: :delete,
                                       confirm: 'Are you sure?',
                                       remote: true,
                                       :class => "del_post" %></td>
    </tr>
  <% end %>